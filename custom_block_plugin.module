<?php

/**
 * HOOK_theme()
 * @param [type] $existing
 * @param [type] $type
 * @param [type] $theme
 * @param [type] $path
 * @return void
 */

function custom_block_plugin_theme($existing, $type, $theme, $path) {
  return array(
    'block__customtemplate' => array(
      'render element' => 'elements',
      'template' => 'block--customtemplate',
      'base hook' => 'block'
    )
  );
}
